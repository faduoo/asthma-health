{
  "name": "asthma-health-backend-visualizer",
  "private": true,
  "version": "1.0.0",
  "description": "Comprehensive asthma management application with health tracking, medication management, and AI assistant",
  "author": "faduoo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/faduoo/asthma-health.git"
  },
  "keywords": [
    "asthma",
    "health",
    "management",
    "tracker",
    "medication",
    "react",
    "typescript",
    "vite"
  ],
  "type": "module",
  "scripts": {
