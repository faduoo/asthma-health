    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "json-server": "^1.0.0-beta.3",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
